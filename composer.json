{
    "name": "task/commission-task",
    "description": "Commission task",
    "keywords": [],
    "type": "project",
    "autoload": {
        "psr-4": {
            "CommissionTask\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CommissionTask\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.0",
        "ext-bcmath": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "curl/curl": "^2.0",
        "friendsofphp/php-cs-fixer": "2.16"
    },
    "config": {
        "sort-packages": true,
        "bin-dir": "bin"
    },
    "scripts": {
        "phpunit": "phpunit",
        "fix-cs": "php-cs-fixer fix",
        "test-cs": "php-cs-fixer fix --dry-run -v",
        "test": ["@phpunit"]
    }
}
